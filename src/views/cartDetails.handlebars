<h1>Carrito</h1>
<table>
    <tr>
        <th>Nombre del producto</th>
        <th>Precio</th>
        <th>Cantidad</th>
    </tr>
    {{#each cart.products}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.unitPrice}}</td>
            <td>{{this.quantity}}</td>
        </tr>
    {{/each}}
</table>
